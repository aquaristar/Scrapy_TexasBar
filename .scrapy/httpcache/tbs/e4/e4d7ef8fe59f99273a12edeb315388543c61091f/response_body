<section data-global-search=".global-search-btn" class="global-search">
	<div class="content-wrapper content-wrap">
	    <div class="global-search-box pull-left">
            <script>
                function postInit() {

                    google.search.cse.element.render(
                    {
                        div: "cse",
                        tag: 'searchbox-only',
                        attributes: {
                            resultsUrl: '/sitesearch',
                            newWindow: 'false',
                            queryParameterName: 'query'
                        }
                    });

                    $('.search').click(function () {
                        var searchpage = '/sitesearch';
                        var query = '?query=' + $('input.gsc-input').val();

                        window.location = searchpage + query;
                    })


                    $(".gsc-input").attr("placeholder", "Search");
                    $(".gsc-input").find(':input[placeholder]').placeholder();


                    $('input.gsc-input').keyup(function (e) {
                        if (e.keyCode == '13') {
                            $('.search').trigger('click');
                        } else if ($('input.gsc-input').val().length > 2) {
                            $.ajax({
                                type: "GET",
                                url: '/api/sitecore/Components/LoadSuggestedResults' + '?query=' + $('input.gsc-input').val(),
                                crossDomain: false
                            }).done(function (data) {
                                $('.suggested-results').html(data);
                            });
                        } else if ($('input.gsc-input').val().length <= 2) {
                            $('.suggested-results').html('');
                        }
                    })
                };                

                window.__gcse = {
                    callback: postInit
                };

	            (function () {
	                var cx =  '001729409881330894987:1k7hyllpubc';
	                var gcse = document.createElement('script');
	                gcse.type = 'text/javascript';
	                gcse.async = true;
	                gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') +
                        '//www.google.com/cse/cse.js?cx=' + cx;
	                var s = document.getElementsByTagName('script')[0];
	                s.parentNode.insertBefore(gcse, s);
	            })();
            </script>
            <gcse:searchbox-only resultsUrl="/sitesearch" newWindow="false" queryParameterName="query"></gcse:searchbox-only>
		    <button class="search"><i class="fa fa-search"></i></button>
	    </div>

	    <div class="pull-left global-search-results">
		    <h3>Suggested Results</h3>
