<script src="/assets/src/js/jquery/jquery.cookie.js" type="text/javascript"></script>
<link rel="stylesheet" type="text/css" href="/assets/src/js/iealert/style.css" />
<script src="/assets/src/js/iealert/iealert.min.js"></script>

<script>
    $(document).ready(function () {
        var hasCookie = $.cookie('bakerbotts-cookie');
        if (hasCookie)
            $("#sliding-popup").hide();
        else
            $("#sliding-popup").addClass('is-animating');

        $(".agree-button").on("click", function () {
            $.cookie("bakerbotts-cookie", 1, { expires: 1000, path: "/" });
            $("#sliding-popup").fadeOut();
        });
        
        var ieAlertCookie = $.cookie('bakerbotts-iealert');
        if (!ieAlertCookie) {
            $("body").iealert({
                support: "ie8",
                title: $(".js-browsernotice-title").val(),
                text: $(".js-browsernotice-text").val(),
                upgradeTitle: $(".js-browsernotice-upgradetitle").val(),
                upgradeLink: $(".js-browsernotice-upgradelink").val()
            });
            $.cookie("bakerbotts-iealert", 1, { expires: 1000, path: "/" });
        }
    });

</script>
        

<script src="/assets/src/js/libs/jquery.placeholder.min.js"></script>
