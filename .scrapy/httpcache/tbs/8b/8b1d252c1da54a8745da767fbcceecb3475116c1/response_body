		<script type="text/javascript" src="https://maps.google.com/maps/api/js?sensor=false"></script>
	
		<script type="text/javascript">
		var address = 'texas (no address provided by attorney)';

		var geocoder3;
		var map3;

		function initializeGoogleMap() {
			geocoder3 = new google.maps.Geocoder();

			if (geocoder3) {
				geocoder3.geocode( { 'address': address }, function(results, status) {
					if (status == google.maps.GeocoderStatus.OK) {
						//var latlng = new google.maps.LatLng(-34.397, 150.644);
						var myOptions = {
							zoom: 4,
							center: results[0].geometry.location,
							draggable: true,
							mapTypeControlOptions: {
								style: google.maps.MapTypeControlStyle.DROPDOWN_MENU
							},
							navigationControl: true,
							navigationControlOptions: {
								style: google.maps.NavigationControlStyle.ZOOM_PAN,
								position: google.maps.ControlPosition.TOP_LEFT
								},
							scaleControl: true,
							mapTypeId: google.maps.MapTypeId.ROADMAP
						};
						map3 = new google.maps.Map(document.getElementById("location-map"), myOptions);
						map3.panBy(25,-55);

						var marker = new google.maps.Marker({
							position: results[0].geometry.location,
							map: map3
						});

						var infoText =  address.replace(/"/g,'');
						var infoWindow = new google.maps.InfoWindow({
							content: infoText
						});
						infoWindow.open(map3, marker);

					} else {
						alert("Geocode was not successful for the following reason: " + status);
					}
				});
			}
		}

		</script>
	





