	var aMainMenuImages = new Array();
	aMainMenuImages[0] = new Image();
	aMainMenuImages[0].src = "./site_images/menuarrow1.gif";
	aMainMenuImages[1] = new Image();
	aMainMenuImages[1].src = "./site_images/menuarrow2.gif";
	function SwapButton(id, state)
	{
		if(id != '1')
		{
			if(state)
				document.getElementById(id + "Button").src='../site_images/menuarrow2.gif';
			else
				document.getElementById(id + "Button").src='../site_images/menuarrow1.gif';
		}
		else
		{
			if(state)
				document.getElementById(id + "Button").src='../site_images/shop_by_brand_over.gif';
			else
				document.getElementById(id + "Button").src='../site_images/shop_by_brand.gif';
		}
	}
	window.onresize=myWindowResize;
	window.onload=myWindowLoad;

function myWindowResize()
{
    cswmRefresh();
}

function myWindowLoad()
{
  cswmMenuBarInit();
}
</script>

